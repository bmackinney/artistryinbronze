{
  "name": "quire-catalogue",
  "version": "0.0.1",
  "description": "This repository contains a starter Hugo site for use with the **Quire** project (a suite of digital publishing tools from Getty Publications). Use this kit to kick off development of a digital art catalogue or similar publication.",
  "main": "index.js",
  "scripts": {
    "build": "node_modules/.bin/gulp build --silent",
    "preview": "node_modules/.bin/gulp dev --silent",
    "deploy": "bin/deploy.sh --silent",
    "setup": "node_modules/.bin/gulp setup --silent"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gettypubs/quire-catalogue.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gettypubs/quire-catalogue/issues"
  },
  "homepage": "https://github.com/gettypubs/quire-catalogue#readme",
  "dependencies": {
    "browser-sync": "^2.18.6",
    "chalk": "^1.1.3",
    "command-exists": "^1.0.2",
    "gulp": "^3.9.1",
    "js-yaml": "^3.7.0"
  }
}
